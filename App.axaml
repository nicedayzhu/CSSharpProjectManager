<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="CSSharpProjectManager.App"
             xmlns:local="using:CSSharpProjectManager"
             xmlns:converters="clr-namespace:CSSharpProjectManager.Converters"
             RequestedThemeVariant="Light">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
    </Application.Styles>
     <Application.Resources>
         <converters:EnumToBooleanConverter x:Key="EnumToBooleanConverter"/>
         <converters:TemplateDescriptionConverter x:Key="TemplateDescriptionConverter"/>
     </Application.Resources>
</Application>